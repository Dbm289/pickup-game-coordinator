<h2>Sign Up!</h2>

<form class="" action="/users" method="post">
    <label for="name">Name: </label>
    <input type="text" name="name" value="">
    <label for="email">Email: </label>
    <input type="text" name="email" value="">
    <label for="password">Password: </label>
    <input type="text" name="password" value="">
    <label for="position">Position: </label>
    <input type="text" name="position" value="">
    <label for="alt_position">Alternate Position: </label>
    <input type="text" name="alt_position" value="">
    <label for="skill_level">Player Skill Level: </label>
    <input type="number" name="skill_level" min="1" max="10">
    <label for="coaching_skill_level">Coaching Skill Level: </label>
    <input type="number" name="coaching_skill_level" min="1" max="10">
    <input type="submit" name="" value="Create Player">



</form>